NET "LPC_CLK" TNM_NET = LPC_CLK;
TIMESPEC TS_LPC_CLK = PERIOD "LPC_CLK" 33 Mhz HIGH 50%;

# BANK[3:0]
# Bank 1 is default on power-up.
NET REG_00EF_WRITE(0) INIT=S;

NET "QPI_IO<0>" LOC = "P1";
