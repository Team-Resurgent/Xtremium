NET "HEADER_MISO1" LOC = "P41";
NET "HEADER_MISO2" LOC = "P36";
NET "HEADER_CS" LOC = "P28";
NET "HEADER_SCK" LOC = "P39";
NET "HEADER_MOSI" LOC = "P40";
